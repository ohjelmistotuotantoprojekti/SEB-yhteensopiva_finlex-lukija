{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "docker-compose -f runDev.yaml up --build --abort-on-container-exit",
    "build": "tsc -b && vite build && rm -rf ../backend/src/frontend && cp -r dist ../backend/src/frontend",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
